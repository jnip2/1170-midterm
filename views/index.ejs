    <!-- JOEY'S NAV BAR -->
    <div class="nav">
        <div class="logo">
            <a href="/"><img src="assets/bear-brown.png" alt="" class="bear"></a>
            <h1><a href="/">Tea n Me</a></h1>
        </div>
        <div class="hamburger" onclick="burgerOn()">
